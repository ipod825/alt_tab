{
  "manifest_version": 3,
  "name": "AltTab",
  "version": "1.0",
  "permissions": [
    "commands",
    "tabs",
    "storage"
  ],
  "commands": {
    "ALTTAB_ActivateLast": {
      "suggested_key": {
        "default": "Alt+W"
      },
      "description": "Activate last active tab."
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
